{
    "name": "genealabs/laravel-messenger",
    "description": "Flash notification module for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "GeneaLabs, LLC",
            "email": "hello@genealabs.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.5 - 5.7",
        "illuminate/session": "5.5 - 5.7",
        "jenssegers/model": "*"
    },
    "require-dev": {
        "illuminate/routing": "5.5 - 5.7",
        "laravel/laravel": "5.5 - 5.7",
        "mockery/mockery": "0.9.*",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "~8.2",
        "satooshi/php-coveralls" : "2.0.*",
        "sebastian/phpcpd": "*"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/laravel/laravel/app/",
            "GeneaLabs\\LaravelMessenger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelMessenger\\Providers\\Service"
            ],
            "aliases": {
                "Messenger": "GeneaLabs\\LaravelMessenger\\Facades\\Messenger"
            }
        }
    },
    "minimum-stability": "stable"
}
